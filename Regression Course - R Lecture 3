    "## Question 2: Write your own F-Test Functin"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "id": "3425a7fa",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-10-31T13:23:06.104732Z",
     "iopub.status.busy": "2025-10-31T13:23:06.103204Z",
     "iopub.status.idle": "2025-10-31T13:23:06.117958Z",
     "shell.execute_reply": "2025-10-31T13:23:06.116197Z"
    },
    "papermill": {
     "duration": 0.029347,
     "end_time": "2025-10-31T13:23:06.120484",
     "exception": false,
     "start_time": "2025-10-31T13:23:06.091137",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "my_f_test <- function(x, y) {\n",
    "    alpha <- 0.05\n",
    "    \n",
    "    n_x <- length(x)\n",
    "    n_y <- length(y)\n",
    "    dof1 <- n_x - 1\n",
    "    dof2 <- n_y - 1\n",
    "    \n",
    "    var_x <- var(x)\n",
    "    var_y <- var(y)           \n",
    "    \n",
    "    f_stat <- var_x/var_y\n",
    "    \n",
    "    \n",
    "    # Compute p-value (two-tailed)\n",
    "    if (f_stat > 1) {\n",
    "        p_value <- 2 * pf(f_stat, dof1, dof2, lower.tail = FALSE)\n",
    "    } else {\n",
    "        p_value <- 2 * pf(f_stat, dof1, dof2, lower.tail = TRUE)\n",
    "    }\n",
    "    \n",
    "    # Ensure p-value does not exceed 1\n",
    "    p_value <- min(p_value, 1)\n",
    "        \n",
    "    # Decision rule\n",
    "    if (p_value < alpha) {\n",
    "        response <- \"Reject Null Hypothesis: Variances are significantly different.\"\n",
    "    } else {\n",
    "        response <- \"Fail to Reject Null Hypothesis: Variances are not significantly different.\"\n",
    "    }\n",
    "    \n",
    "    # Return results as a list\n",
    "    return(list(\n",
    "        paste(\"Computed F-statistic:\", round(f_stat, 4)),\n",
    "        paste(\"Degrees of Freedom: df1 =\", dof1, \", df2 =\", dof2),\n",
    "        paste(\"p-value:\", format(p_value, scientific = TRUE, digits = 4)),\n",
    "        paste(\"Response:\", response)\n",
    "    ))\n",
    "}\n"
   ]
  },
  {
